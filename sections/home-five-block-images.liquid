                   data-src="{{ section.settings.right_img_3 | img_url: '540x' }}"
                   data-widths="[180, 360, 540, 720, 900, 1080, 1296, 1512, 1728, 2048]"
                   data-aspectratio="{{ section.settings.right_img_3 | img_url: 'master' }}"
