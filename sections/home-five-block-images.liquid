<section class="five-block-image">
  <div class="container">
    <div class="row">


      {% if section.settings.dpl_clmn_right %}
      <div class="column-right {% if section.settings.dpl_clmn_left %}col-md-6 {% else %}col-md-12{% endif %}">
        <div class="row">
          {% if section.settings.dpl_right_img_1 %}
          <figure class="img-itm col-6">
            <a href="{{ section.settings.right_link_1 }}" title="{{ section.settings.right_hd_1 }}">            
              {% if section.settings.right_img_1 != blank %}
              <div class="p1">
              <img data-new-src="{{ section.settings.right_img_1 | img_url: '2x'}}" alt="{{section.settings.right_img_1.alt}}"
                   class="lazyload zoom lazy"
                   src="{{ section.settings.right_img_1 | img_url: '54x' }}"
                   data-src="{{ section.settings.right_img_1 | img_url: '540x' }}"
                   data-widths="[180, 360, 540, 720, 900, 1080, 1296, 1512, 1728, 2048]"
                   data-aspectratio="{{ section.settings.right_img_1 | img_url: 'master' }}"
                   data-sizes="auto">
              </div>
              {% else %}
              <span class="not_img">270 x 270px</span>
              {% endif %}
              <figcaption>              
                {% include 'multilang' with section.settings.right_hd_1 %}            
              </figcaption>            
            </a>
          </figure>
          {% endif %}

          {% if section.settings.dpl_right_img_2 %}
          <figure class="img-itm col-6">
            <a href="{{ section.settings.right_link_2 }}" title="{{ section.settings.right_hd_2 }}">            
              {% if section.settings.right_img_2 != blank %}
              <div class="p1">
              <img data-new-src="{{ section.settings.right_img_2 | img_url: '2x'}}" alt="{{section.settings.right_img_2.alt}}"
                   class="lazyload zoom lazy"
                   src="{{ section.settings.right_img_2 | img_url: '54x' }}"
                   data-src="{{ section.settings.right_img_2 | img_url: '540x' }}"
                   data-widths="[180, 360, 540, 720, 900, 1080, 1296, 1512, 1728, 2048]"
                   data-aspectratio="{{ section.settings.right_img_2 | img_url: 'master' }}"
                   data-sizes="auto">
              </div>
              {% else %}
              <span class="not_img">270 x 270px</span> 
              {% endif %}
              <figcaption>              
                {% include 'multilang' with section.settings.right_hd_2 %}            
              </figcaption>            
            </a>
          </figure>
          {% endif %}

          {% if section.settings.dpl_right_img_3 %}
          <figure class="img-itm col-6">
            <a href="{{ section.settings.right_link_3 }}" title="{{ section.settings.right_hd_3 }}">            
