            <figcaption>              
              {% include 'multilang' with section.settings.left_hd_3 %}            
            </figcaption>            
          </a>
        </figure>
        {% endif %}

      </div>
      {% endif %}
      
    </div>
  </div>
</section>

<style>
  .five-block-image .column-left .img-itm figcaption{
    color:{{ section.settings.color_header }};
    font-size: {{ section.settings.font_size_header }};
  }

  .five-block-image .column-right .img-itm figcaption{
    color:{{ section.settings.color_header_2 }};
    font-size: {{ section.settings.font_size_header_2 }};
    
  }
  
  .five-block-image .img-itm figcaption span:before{
    background:{{ section.settings.color_header }};
  }
  
  .five-block-image .column-left > .img-itm:not(:first-child){
    display:none;
  }
  
       .zoom{
transition: transform .4s ease !important;
}
.p1 {
    overflow: hidden !important;
}
  
  
  figure.img-itm.col-6:hover img{
    transform: scale(1.1) !important;
}
  
  figure.img-itm.slick-slide.slick-current.slick-active:hover img{
  transform: scale(1.1) !important;
  }
</style>

