            {% else %}
            <span class="not_img">570 x 570px</span>
            {% endif %}
            <figcaption>              
              {% include 'multilang' with section.settings.left_hd_1 %}            
            </figcaption>            
          </a>
        </figure>
        {% else %}
        <div>
         <video class="img-itm" style="width: 100%;height: 100%;" loop autoplay playsinline muted preload="metadata">
          <source src="{{section.settings.vedio_url}}" type="video/mp4">

        </video>
        </div>
        {% endunless %}
        {% endif %}

        {% if section.settings.dpl_left_img_2 %}
