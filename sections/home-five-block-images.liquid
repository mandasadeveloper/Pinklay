            <div class="p1">
            <img data-new-src="{{ section.settings.left_img_2 | img_url: '2x'}}" alt="{{ section.settings.left_hd_2 | escape }}"
                 class="lazyload zoom lazy"
                 src="{{ section.settings.left_img_2 | img_url: '180x' }}"
                 data-src="{{ section.settings.left_img_2 | img_url: 'master' }}"
                 data-widths="[180, 360, 540, 720, 900, 1080, 1296, 1512, 1728, 2048]"
