{% comment %}<!-- Mega menu style 1 -->{% endcomment %}
<style>
  .col6{
        -webkit-flex: 0 0 16.66%;
    -ms-flex: 0 0 16.66%;
    flex: 0 0 16.66%;
    max-width: 16.66%;
  }
</style>
{% if block_style == "style_1" %}
<div class="col-12 {% if dpl_column_first_style and dpl_column_second_style %}column-left col-md-12 col-lg-7 {% elsif dpl_column_first_style == false and dpl_column_second_style == false  %}col-md-12 {% else %}col-md-12 col-lg-9{% endif %}">
  <div class="row">    
    {% for l in linklists[link.handle].links %}
    <div class="inner col-12 {% if dpl_column_first_style == false and dpl_column_second_style == false  %}col-md-12 col-lg-3 {% else %}col-md-12 col-lg-4{% endif %}">
      
      {% comment %}<!--  Menu level 2   -->{% endcomment %}

      <a href="{{ l.url }}" class="current second-menu">
        {% include 'multilang' with l.title %}
        
        {% if linklists[l.handle] != empty %}
        <span class="icon-dropdown">
          <i class="fa fa-angle-right" aria-hidden="true"></i>
        </span>
        {% endif %}
      </a>

      {% comment %}<!--  Menu level 3   -->{% endcomment %}

      {% if linklists[l.handle] != empty %}
      <ul class="dropdown dropdow-lv2">
        <li class="menu-mb-title" style="display: none;">
          <span class="icon-dropdown no-event">
           <i class="fa fa-angle-left" aria-hidden="true"></i>
          </span>
          {% include 'multilang' with l.title %}
        </li>
        
        {% for child_l in linklists[l.handle].links %}
        <li>
          <a class="third-menu {% if child_l.active %}current{% endif %}" href="{{ child_l.url }}">
            {% if section.settings.enable_new %}
              {% assign c_new_item = section.settings.new_item | split:"," | strip | downcase | handle %}
              {% assign c_new = child_l.title | downcase | handle  %}
            {% endif %}
            {% include 'multilang' with child_l.title %}
          </a>
        </li>
        {% endfor %}
      </ul>
      {% endif %}
    </div>
    {% endfor %}
  </div>
</div>

<div class="col-12 {% if dpl_column_first_style and dpl_column_second_style %}column-right col-md-12 col-lg-5 {% else %}col-md-12 col-lg-3 {% endif %}">
  {% if dpl_column_first_style %}
  {% if featured_product != blank %}
  <div class="featured-product">
    {% if title_featured_product != blank %}
    <h3>
      {% include 'multilang' with title_featured_product %}
    </h3>
    {% endif %}
    {% include 'mega-menu-product-grid' %}
  </div>
  {% endif %}
  {% endif %}

  {% if dpl_column_second_style %}
  {% if banner_first_style != blank %}
  <div class="banner">
    <a href="{{ link_banner_first_style }}" title="">    
      <img src="{{ banner_first_style | img_url : "210x"  }}" alt=""
           class="lazyload"
           data-src="{{ banner_first_style | img_url : "210x"}}"
           data-widths="[180, 360, 540, 720, 900, 1080, 1296, 1512, 1728, 2048]"
           data-aspectratio="{{ banner_first_style.aspect_ratio }}"
           data-sizes="auto">
    </a>
  </div>
  {% endif %}
  {% endif %}
</div>
{% endif %}


