    display: -ms-flexbox;
    display: -webkit-flex;
    > div{
      text-transform: uppercase;
      letter-spacing: 0.52px;
      font-size: 13px;
      font-weight: 600;
      color: #323232;
      display: flex;
      display: -ms-flexbox;
      display: -webkit-flex;
    }
    .pro-img{
      padding: 0 15px;
      width:100%;
    }
  }

  .product_order{
    .product-order-form:after,
    .grid-item:after{
      display:block; 
      clear: both;
      content: "";
    }
    .pro-img{
      padding: 15px;
      width:100%;
    }
    .product-bottom{
      .product-title{
        margin:0px;
      }
    }
    .price-box{
      margin: 0px;
      p{
        margin:0;
      }

      span{
        font-size: 14px;
        color: $color_price; 
        font-weight: 500;
        display: inline-block;
        vertical-align: middle;
      }

      .old-price {
        margin-right:5px;

        span{
          color:$color_compare_price;
          text-decoration: line-through;
        }
      }

      .special-price{
        span{
          color: $color_price_sale; 
        }
      }

      em{
        font-size: 12px;
        color: $color_compare_price;
        font-weight: 400;
        font-style: normal;
        margin-right: 3px;
      }
    }
    .options-items,
    .pro-qty{
      .qty-group {
        border: 1px solid #dcdcdc;
        @include border-radius(1px);
        max-width: 90px;
        @include flex(center, center);
        font-size: 12px;
        height: 36px;
        min-width: 90px;
        background: #fff;
        input[type=text] {
          border: none;
          color: #646464;
          padding: 0;
          width: 60px;
          overflow: hidden;
          text-overflow: ellipsis;
          text-align: center;
          outline: none;
        }
      }
      .button{
        width:100%;
        max-width: 36px;
        text-align: center;
        font-size: 16px;
        @include flex(center, center);
        border:none;
        color:#7b7b7b;
        position: relative;
        height: 100%;

        &:before{               
          content: "";
          width: 2px;
          height: 12px;
          background: #7b7b7b;
          display: block;
          position: absolute;
          top: 50%;
          left: 50%;
          -webkit-transform: translateY(-50%) translateX(-50%);
          -ms-transform: translateY(-50%) translateX(-50%);
          transform: translateY(-50%) translateX(-50%);
        }

        &:after{
          content: "";
          width: 12px;
          height: 2px;
          background: #7b7b7b;
          display: block;
          position: absolute;
          top: 50%;
          left: 50%;
          -webkit-transform: translateY(-50%);
          -ms-transform: translateY(-50%);
          transform: translateY(-50%);
          -webkit-transform: translateX(-50%) translateY(-50%);
          -ms-transform: translateX(-50%) translateY(-50%);
          transform: translateX(-50%) translateY(-50%);
        }

        &.dec{
          &:before{               
            background: transparent;

          }
        }
      }
    }
    .product-des-order{

      .action{
        display:block;
        opacity:1;
      }
    }
  }

  .options-action,
  .action{
    .ajaxified-cart-feedback{
      color: #848484;
      font-weight: 400;
      margin-top:8px;
    }

    .btn{
      cursor: pointer;
      font-size: 13px;
      font-weight: 500;
      text-transform: uppercase;
      letter-spacing: 0.52px;
      min-height: 36px;

      &:disabled{
