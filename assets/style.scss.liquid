%hover_img{
  display: block;
  position: relative;
  overflow: hidden;

  &::before{
    content: "";
    background-color: rgba(255, 255, 255, .10);
    display: block;
    position: absolute;
    left: -100%;
    top: 0;
    width: 100%;
    height: 100%;
    transition: all .3s ease 0s;
    z-index: 1;


  }

  &:hover::before{
    left: 0;
  }
}

@mixin trans_form($y) {
  -webkit-transform: translateY(-$y);
    -ms-transform: translateY(-$y);
      transform: translateY(-$y);
        }

@mixin flex($align-items,$justify-content) {
  display: flex;
  display: -ms-flexbox;
  display: -webkit-flex;
  -webkit-align-items: $align-items;
  -moz-align-items: $align-items;
  -ms-align-items: $align-items;
  align-items: $align-items;  
  -webkit-box-pack: $justify-content;
  -moz-box-pack: $justify-content;
  -ms-flex-pack: $justify-content;
  -webkit-justify-content: $justify-content;
  justify-content: $justify-content;
}

@mixin slick_arrow($width, $height, $background, $color, $top, $left) {
  width: $width;
  height:$height;
  background: $background;
  border:1px solid $background;
  outline: none!important;
  position: absolute;
  top:$top;
  left: $left;
  cursor: pointer;
  z-index: 2;

  svg{
    display: flex;
    align-items: center;
    justify-content: center;
    fill: $color;
  }
}

@mixin border-radius($radius) {
  -moz-border-radius: $radius;
  -webkit-border-radius: $radius;
  -ms-border-radius: $radius;
  -o-border-radius: $radius;
  border-radius: $radius;
}

@mixin slick_dots($al, $w, $h, $bg, $bd, $act){
  .slick-dots{
    padding: 0;
    margin-top: 10px;
    text-align: $al;

    li{
      list-style: none;
      margin: 5px; 
      display: inline-block;

      button{
        width: $w;
        height: $h;
        font-size: 0;
        background: $bg;
        border:$bd solid $bg;
        border-radius: 100%;
        line-height: normal;
        padding: 0;
        outline: none;
        cursor: pointer;
      }

      &.slick-active{
        button{
          background: $act;
        }
      }
    }
  }
}

@mixin overlay{
  position: fixed;
  height: 100%;
  width: 100%;
  background: #000;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  opacity: .2;
  transition: all .2s ease;
  -webkit-transition: all .2s ease;
  overflow: hidden;
}

